<section>


<div>
	<center><h2>Solicitudes de interconsultas por establecimiento APS</h2></center>
	<table class = "listing" >
		<tr class = "header">
			<th>ID Usuario</th>
			<th>ID Paciente</th>
			<th>Nombres</th>
			<th>Apellido Paterno</th>
			<th>Apellido Materno</th>
			<th>Prevision</th>
			<th>Establecimiento APS</th>
			<th>Solicitud de interconsulta</th>
		</tr>

		<% @pacientes_horas.each do |phr| %>
			<% @prevision.each do |prev| %>	
				<% @horas_especifico.each do |aspr| %>
					<% if prev.patient_id == phr.patient_id %>
						<% if aspr.patient_id == phr.patient_id %>
							<% if aspr.RH_ID == phr.RH_ID %>
							<tr>
								<td><%= phr.user_id %></td>
								<td><%= phr.patient_id %></td>
								<td><%= phr.user_NAMES %></td>
								<td><%= phr.user_LASTNAME1 %></td>
								<td><%= phr.user_LASTNAME2 %></td>
								<td><%= prev.prevision_NAME %></td>
								<td><%= aspr.EST_ADDRESS %></td>
								<td><%= aspr.RH_DERIVED %></td>
	
							</tr>
							<% end %>
						<% end %>
					<% end %>	
				<% end %>	
			<% end %>	
		<% end %>
		

		


	</table>
</div>

</section>